;-------------------------------------------------------------
;--------------------音乐频谱设置-------------------------
;-------------------------------------------------------------
;音乐频率，越大消耗cpu性能越高
FFTSize=2048
;不知道是啥
FFTOverlap=1024
;频谱跳起所需时间（ms）
FFTAttack=650
;频谱收缩所需的时间（ms）
FFTDecay=250
;最小频段（Hz）
FreqMin=100
;最大频段（Hz）
FreqMax=16000
;频谱对音量的敏感度
Sensitivity=45
;播放器类型,支持输入：网易云音乐、QQ音乐、酷狗音乐、酷我音乐
PlayerName=CloudMusic                    
